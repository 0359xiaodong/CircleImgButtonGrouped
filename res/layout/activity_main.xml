<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res/com.edp.circlebuttongrouped"
    android:id="@+id/rlRoot"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:padding="10dp"
    tools:context=".MainActivity" >

    <RelativeLayout
        android:id="@+id/rlLeft"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1" >

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/cibTop3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            app:collapseAtClick="false"
            app:ibCount="3"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/CibGroup01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/cibTop3"
            android:layout_below="@+id/cibTop3"
            android:layout_marginTop="10dp"
            app:collapseAtClick="false"
            app:ibCount="3"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book"
            app:imgSrcCIB4="@drawable/cry" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/CibGroup02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/CibGroup01"
            android:layout_below="@+id/CibGroup01"
            android:layout_marginTop="10dp"
            app:collapseAtClick="false"
            app:ibCount="5"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book"
            app:imgSrcCIB4="@drawable/cry" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/CibGroup03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/CibGroup01"
            android:layout_below="@+id/CibGroup02"
            android:layout_marginTop="10dp"
            app:collapseAtClick="false"
            app:ibCount="4"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book"
            app:imgSrcCIB4="@drawable/cry"
            app:imgSrcCIB5="@drawable/morning" />
        
        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/cibBottom3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/CibGroup01"
            android:layout_below="@+id/CibGroup03"
            android:layout_marginTop="10dp"
            app:collapseAtClick="false"
            app:ibCount="3"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book"
            app:imgSrcCIB4="@drawable/cry"
            app:imgSrcCIB5="@drawable/morning" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/cibTop4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/CibGroup01"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/cibTop3"
            app:collapseAtClick="false"
            app:ibCount="4"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/cibTop5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/cibTop4"
            app:collapseAtClick="false"
            app:ibCount="5"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/cibTop6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/cibTop5"
            app:collapseAtClick="false"
            app:ibCount="6"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/cibBottom5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/cibTop5"
            android:layout_alignTop="@+id/cibBottom4"
            app:collapseAtClick="false"
            app:ibCount="5"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/cibBottom4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/cibTop4"
            android:layout_alignTop="@+id/cibBottom3"
            app:collapseAtClick="false"
            app:ibCount="4"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book" />

        <com.edp.circlebuttongrouped.CircleImgBtnGroup
            android:id="@+id/cibBottom6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/cibTop6"
            android:layout_alignTop="@+id/cibBottom5"
            app:collapseAtClick="false"
            app:ibCount="6"
            app:ibHeight="80"
            app:ibWidth="80"
            app:imgSrcCIB1="@drawable/afternoon"
            app:imgSrcCIB2="@drawable/close_activity"
            app:imgSrcCIB3="@drawable/close_book" />
        
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rlRight"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1" >

    </RelativeLayout>

</LinearLayout>
