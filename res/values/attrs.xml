<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleImgBtn">
        <attr name="height" format="integer"></attr>
        <attr name="width" format="integer"></attr>
        <attr name="border" format="boolean"></attr>
        <attr name="border_width" format="integer"></attr>
        <attr name="border_color" format="color"></attr>
        <attr name="shadow" format="boolean"></attr>
    </declare-styleable>
    
    <declare-styleable name="CircleImgBtnGroup">
        <attr name="ibHeight" format="integer"></attr>
        <attr name="ibWidth" format="integer"></attr>
        <attr name="ibCount" format="integer"></attr>
        <attr name="label" format="string"></attr>
        <attr name="inverted" format="boolean"></attr>
        <attr name="collapseAtClick" format="boolean"></attr>
        <attr name="sendBackAtClick" format="boolean"></attr>
        <attr name="timeoutToCollapse" format="integer"></attr>
        <attr name="imgSrcCIB1" format="reference"></attr>
        <attr name="imgSrcCIB2" format="reference"></attr>
        <attr name="imgSrcCIB3" format="reference"></attr>
        <attr name="imgSrcCIB4" format="reference"></attr>
        <attr name="imgSrcCIB5" format="reference"></attr>
        <attr name="imgSrcCIB6" format="reference"></attr>
    </declare-styleable>
            
